{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "red": "#ed8796",
    "green": "#a6da95",
    "blue": "#8aadf4",
    "yellow": "#eed49f",
    "magenta": "#c6a0f6",
    "cyan": "#8bd5ca",
    "ruby": "#cc342d",
    "crystal": "#000000",
    "rust": "#ea4800",
    "julia": "#495fa5",
    "python": "#f2c337",
    "node": "#7bc326",
    "deno": "#1f23af"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:blue",
          "template": "@{{ .UserName }}",
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "p:green",
          "template": " \u279c ",
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "p:magenta",
          "properties": {
            "style": "agnoster_short"
          },
          "template": "{{ .Path }}",
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "p:yellow",
          "template": " \uf119",
          "style": "plain",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "p:yellow",
          "template": "{{ .FormattedMs }}",
          "style": "plain",
          "type": "executiontime"
        },
        {
          "foreground": "p:cyan",
          "template": "{{ if .Error }}{{ .Error }}{{ else }} {{ .Name }}{{ if eq .Version \"0.0.0\" }}{{ else }}@{{ .Version }}{{ end }}{{ end }}",
          "style": "plain",
          "type": "project"
        },
        {
          "foreground": "p:ruby",
          "foreground_templates": [
            "{{if .Error }}{{ p:red }}{{ end }}"
          ],
          "template": "{{ if .Error }}{{ .Error }}{{ else }} .rb@{{ .Full }}{{ end }}",
          "style": "plain",
          "type": "ruby"
        },
        {
          "foreground": "p:crystal",
          "foreground_templates": [
            "{{if .Error}}{{ p:red }}{{ end }}"
          ],
          "template": "{{ if .Error }}{{ .Error }}{{ else }} .cr@{{ .Full }}{{ end }}",
          "style": "plain",
          "type": "crystal"
        },
        {
          "foreground": "p:rust",
          "foreground_templates": [
            "{{if .Error }}{{ p:red }}{{ end }}"
          ],
          "template": "{{ if .Error }}{{ .Error }}{{ else }} .rs@{{ .Full }}{{ end }}",
          "style": "plain",
          "type": "rust"
        },
        {
          "foreground": "p:julia",
          "foreground_templates": [
            "{{if .Error }}{{ p:red }}{{ end }}"
          ],
          "template": "{{ if .Error }}{{ .Error }}{{ else }} .jl@{{ .Full }}{{ end }}",
          "style": "plain",
          "type": "julia"
        },
        {
          "foreground": "p:python",
          "foreground_templates": [
            "{{if .Error }}{{ p:red }}{{ end }}"
          ],
          "template": "{{ if .Error }}{{ .Error }}{{ else }} .py@{{ .Full }}{{ end }}",
          "style": "plain",
          "type": "python"
        },
        {
          "foreground": "p:node",
          "foreground_templates": [
            "{{if .Error }}{{ p:red }}{{ end }}"
          ],
          "template": "{{ if .Error }}{{ .Error }}{{ else }} .js@{{ .Full }}{{ end }}",
          "style": "plain",
          "type": "node"
        },
        {
          "foreground": "p:deno",
          "foreground_templates": [
            "{{if .Error }}{{ p:red }}{{ end }}"
          ],
          "properties": {
            "display_mode": "environment"
          },
          "template": "{{ if .Error }}{{ .Error }}{{ else }} .ts@{{ .Full }}{{ end }}",
          "style": "plain",
          "type": "deno"
        },
        {
          "foreground": "p:yellow",
          "properties": {
            "branch_icon": "",
            "branch_identical_icon": "",
            "fetch_status": true
          },
          "template": " git(<p:cyan>{{ .HEAD }}{{ if or (.Working.Changed) (.Staging.Changed) }}*{{ end }}{{ if .BranchStatus }}{{ .BranchStatus }}{{ end }}</>)",
          "style": "plain",
          "type": "git"
        }
      ],
      "type": "rprompt"
    }
  ],
  "final_space": true,
  "version": 2
}